{
  "name": "phonenum-generator",
  "version": "1.0.0",
  "description": "an api to generate random phone numbers",
  "main": "./dist/app.js",
  "repository": "https://github.com/rickynyairo/phonenum-generator",
  "author": "rickynyairo <rickynyairo@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "tsc-watch --onSuccess \"node ./dist/app.js\"",
    "start:node": "node ./dist/app.js",
    "lint": "tslint 'src/**/*.ts' --fix"
  },
  "engines": {
    "node": ">=10.0.0 <=11.4.0",
    "npm": ">=6.0.0 <7.0.0 "
  },
  "lint-staged": {
    "*.ts": [
      "tslint src --fix",
      "git add"
    ],
    "*.json": [
      "tslint src --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint"
    }
  },
  "dependencies": {
    "@types/compression": "^0.0.36",
    "@types/cors": "^2.8.5",
    "@types/express": "^4.17.0",
    "@types/node": "^12.6.2",
    "@types/swagger-ui-express": "^3.0.1",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "5.0.0-alpha.7",
    "swagger-ui-express": "^4.0.7",
    "tsc-watch": "^2.2.1",
    "typescript": "^3.5.3"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "dotenv": "^8.0.0",
    "husky": "^3.0.0",
    "lint-staged": "^9.2.0",
    "tslint": "^5.18.0"
  }
}
